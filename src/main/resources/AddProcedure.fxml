<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" fx:controller="main.Controllers.Admin_AddProcedure" style="-fx-background-color: #BBD4FC94;" stylesheets="@AdminStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>

      <ImageView layoutX="237.0" layoutY="14.0">
         <image>
            <Image url="@images/logo.png" />
         </image>
      </ImageView>

      <TextField fx:id="textFieldTittle" layoutX="111.0" layoutY="169.0" prefHeight="26.0" prefWidth="378.0" promptText="Название" />
      <TextArea fx:id="textAreaDescription" layoutX="111.0" layoutY="208.0" prefHeight="200.0" prefWidth="378.0" promptText="Описание услуги" />
      <TextField fx:id="textFieldPrice" layoutX="111.0" layoutY="416.0" prefHeight="26.0" prefWidth="164.0" promptText="стоимость услуги (BYN)" />
      <TextField fx:id="textFieldTime" layoutX="299.0" layoutY="416.0" prefHeight="26.0" prefWidth="189.0" promptText="длительность услуги (минуты)" />


      <Button fx:id="ButtonAdd" onAction="#Add_Pressed" layoutX="111.0" layoutY="462.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="378.0" styleClass="button" text="добавить услугу" textFill="WHITE" />
      <Button fx:id="ButtonExit" onAction="#Exit_Pressed" layoutX="110.0" layoutY="513.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="378.0" styleClass="button" text="вернуться назад" textFill="WHITE" />

   </children>
</AnchorPane>
